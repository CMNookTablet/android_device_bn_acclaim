# mount point	fstype    device    	[device2]
/boot		    emmc	  /dev/block/platform/mmci-omap-hs.1/by-name/boot
/bootdata       vfat      /dev/block/platform/mmci-omap-hs.1/by-name/bootdata
/bootloader     emmc      /dev/block/platform/mmci-omap-hs.1/by-name/bootloader
/cache      	ext4      /dev/block/platform/mmci-omap-hs.1/by-name/cache
/data      	 	ext4      /dev/block/platform/mmci-omap-hs.1/by-name/userdata
/emmc    	 	vfat      /dev/block/platform/mmci-omap-hs.1/by-name/media
/recovery	    emmc	  /dev/block/platform/mmci-omap-hs.1/by-name/recovery
/rom            vfat      /dev/block/platform/mmci-omap-hs.1/by-name/rom
/system    	 	ext4      /dev/block/platform/mmci-omap-hs.1/by-name/system
/xloader	    emmc	  /dev/block/platform/mmci-omap-hs.1/by-name/xloader
/sdcard    	 	vfat      /dev/block/mmcblk1p1 /dev/block/mmcblk1
/sd-ext     	ext4      /dev/block/mmcblk1p2